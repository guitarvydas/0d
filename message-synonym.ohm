MessageSynonym {
  top = spaces matchLR spaces matchRL spaces

  bracket =
    | "(" | ")" | "{" | "}" | "[" | "]" | "<" | ">"
    |  "❲" |  "❳" |  "«" | "»" | "⟨" |  "⟩" | "⟪" | "⟫"

   x = spaces char+
   char =  ~space ~bracket any

   matchLR =
     | space+ -- atLeastOneSpace
     | patternLR -- pattern
     | char+ -- char
   patternLR = spaces "⟪" ilr ilr ilr ilr"⟫" spaces
   ilr = spaces matchLR
   
   matchRL =
     | space+ -- atLeastOneSpace
     | patternRL -- pattern
     | char -- char
   patternRL = "⟨" x x x x x "⟩"
   
}
